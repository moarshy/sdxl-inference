{
    "machine": {
        "gpu": 1,
        "cpu": 1000,
        "ram": 100
    },
    "job": {
        "APIVersion": "V1beta1",
        "Metadata": {
            "CreatedAt": "0001-01-01T00:00:00Z",
            "Requester": {}
        },
        "Spec": {
            "Deal": {
                "Concurrency": 1
            },
            "Docker": {
                "Entrypoint": [
                    "python3",
                    "/app/inference.py",
                    "--prompt",
                    {{.Prompt}},
                    "--num_inf_steps",
                    {{.Steps}},
                    "--lora_model",
                    "/inputs/outputs"
                ],
                "Image": "docker.io/marshath/sdxl-inference:v0.0.2"
            },
            "Engine": "Docker",
            "Language": {
                "JobContext": {}
            },
            "Network": {
                "Type": "None"
            },
            "PublisherSpec": {
                "Type": "Estuary"
            },
            "Resources": {
                "GPU": "1"
            },
            "Timeout": 1800,
            "Verifier": "Noop",
            "Wasm": {
                "EntryModule": {}
            },
            "inputs": [
                {
                    "CID": {{.LoraCID}},
                    "Name": "lora_input",
                    "StorageSource": "IPFS",
                    "path": "/inputs"
                }
            ],
            "outputs": [
                {
                    "Name": "outputs",
                    "StorageSource": "IPFS",
                    "path": "/outputs"
                }
            ]
        }
    }
}
